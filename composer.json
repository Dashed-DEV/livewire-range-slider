{
    "name": "dashed/livewire-range-slider",
    "description": "A Tall Stack wrapper for noUiSlider Range Slider",
    "keywords": [
        "jantinnerezo",
        "livewire-range-slider"
    ],
    "homepage": "https://github.com/Dashed-DEV/livewire-range-slider",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Robin van Maasakker",
            "email": "robin@dashed.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.14",
        "phpunit/phpunit": "^10.4",
        "livewire/livewire": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Dashed\\LivewireRangeSlider\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dashed\\LivewireRangeSlider\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dashed\\LivewireRangeSlider\\LivewireRangeSliderServiceProvider"
            ],
            "aliases": {
                "LivewireRangeSlider": "Dashed\\LivewireRangeSlider\\LivewireRangeSliderFacade"
            }
        }
    }
}